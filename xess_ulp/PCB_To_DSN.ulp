          {
            Board.signals(Net)
		        {     
              Net.polygons(Poly)
              {
                if (Poly.layer == Layer.number)
                {
                  if (IsSignal)
                  {
                    printf(" (type power)");
                    printf(" (use_net ");
                    printf(Net.name);
                  }
                  else
                  {               
                    printf(" ");
	                  printf(Net.name);
                  }
                  IsSignal = 0;
                }
              }      
		        }
          }

          if (IsSignal)
          {
            if (LyrType[Layer.number-1] == 1)
              printf(" (type power)");
            else
